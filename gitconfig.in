[user]
	name = Peter Burkholder
	email = peter.burkholder@gsa.gov

[alias]
   co = checkout
   poh = push origin HEAD
   cam = commit -am

[color]
    ui = true

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = red
[diff]
	noprefix = true
