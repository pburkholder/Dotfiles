[user]
	name = Peter Burkholder
	email = peter.burkholder@gsa.gov

[alias]
   co = checkout
   poh = push origin HEAD
   cam = commit -am

[color]
    ui = true

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = red
[diff]
    noprefix = true
[core]
	hooksPath = /Users/peterburkholder/.git-support/hooks
[gitseekret]
	version = 1
	rulespath = /Users/peterburkholder/.git-support/seekret-rules
	rulesenabled = aws.secret_access_key,aws.access_key_id,aws.account_id,mandrill.username,mandrill.password,mandrill.api_key,newrelic.license_key,slack.api_token
	exceptionsfile = ""
